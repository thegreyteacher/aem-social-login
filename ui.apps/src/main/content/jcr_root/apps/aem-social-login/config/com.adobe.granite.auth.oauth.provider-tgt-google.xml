<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="sling:OsgiConfig"
          oauth.config.id="tgt-google"
          oauth.config.provider.id="thegreyteacher-google-provider"
          oauth.client.id="enter-google-app-id-here"
          oauth.client.secret="enter-google-app-secret-here"
          oauth.scope="[https://www.googleapis.com/auth/userinfo.email]"
          oauth.create.users="{Boolean}true"
          force.strict.username.matching="{Boolean}true"
          oauth.userid.property=""
          oauth.encode.userids="{Boolean}false"
          oauth.hash.userids="{Boolean}false"
          oauth.callBackUrl=""
          oauth.access.token.persist="{Boolean}true"
          oauth.access.token.persist.cookie="{Boolean}false"
          oauth.csrf.state.protection="{Boolean}true"
          oauth.redirect.request.params="{Boolean}false"
          oauth.config.siblings.allow="{Boolean}false"
/>
